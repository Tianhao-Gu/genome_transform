{   "ver": "1.0.0",

    "authors": [
        "janaka","chenry"
    ],
    "contact": "help@kbase.us",
    "visible": false,
    "categories": ["inactive","upload"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [
        {
            "id" : "fasta_file_path",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "/data/bulktest/data/bulktest/janakakbase/fasta/" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        },
        {
            "id" : "genome_id",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ],
                "is_output_name" : true
            }
        },
        {
            "id" : "contigset_id",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ],
                "is_output_name" : true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "genome_transform",
            "method": "fasta_to_contig",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "genome_id",
                    "target_property": "genome_id"
                },
                {
                    "input_parameter": "contigset_id",
                    "target_property": "contigset_id"
                },
                {
                    "input_parameter": "fasta_file_path",
                    "target_property": "fasta_file_path"
                }
            ],
            "output_mapping": [
                {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
                },
                {
                    "input_parameter": "fasta_file_path",
                    "target_property": "id"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}