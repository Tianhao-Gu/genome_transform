{   "ver": "1.0.0",

    "authors": [
        "janaka","chenry"
    ],
    "contact": "help@kbase.us",
    "visible": false,
    "categories": ["active","upload"],
    "widgets": {
        "input": null,
        "output": "kbaseGenomeView"
    },
    "parameters": [
        {
            "id" : "genbank_file_path",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "textsubdata",
            "textsubdata_options" : {
              "subdata_selection": {
                "service_function": "ftp_service.list_files",
                "service_version": "dev",
                "parameter_id" : "genbank_file",
                "subdata_included" : ["data/col_ids"],
                "path_to_subdata": ["data","col_ids"]
              },
              "multiselection":false,
              "show_src_obj":false,
              "allow_custom":false
            }
        },
        {
            "id" : "html_link",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ],
                "is_output_name" : true
            }
        },
        {
            "id": "genome_id",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomes.Genome"],
                "is_output_name":true
            }
        }

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "genome_transform",
            "method": "genbank_to_genome",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "genome_id",
                    "target_property": "genome_id"
                },
                {
                    "input_parameter": "html_link",
                    "target_property": "html_link"
                },
                {
                    "input_parameter": "genbank_file_path",
                    "target_property": "genbank_file_path"
                }
            ],
            "output_mapping": [
                {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
                },
                {
                  "constant_value": "KBaseGenomes.Genome",
                  "target_property": "type"
                },
                {
                  "input_parameter": "genome_id",
                  "target_property": "id"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}